{
  "name": "curso-de-webpack",
  "version": "1.0.0",
  "description": "Este es un curso para aprender webpack",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack index.js -o bundle.js --mode development",
    "build:local": "webpack --mode development",
    "build:external": "webpack --config ./external/webpack.config.js --mode development",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js --mode development",
    "build:extract:css": "webpack --config ./plugin-extract-text/webpack.config.js",
    "build:multi": "webpack --config ./multiples-entry-points/webpack.config.js",
    "build:dev": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js",
    "build:babel": "webpack --mode development --config ./babel-loader/webpack.config.js",
    "build:images": "webpack --mode development --config ./url-loader-images/webpack.config.js",
    "build:fonts": "webpack --mode development --config ./url-loader-fonts/webpack.config.js",
    "build:video": "webpack --mode development --config ./url-loader-video/webpack.config.js",
    "build:json": "webpack --mode development --config ./json-loader/webpack.config.js",
    "build:react": "webpack --mode development --config ./react/webpack.config.js",
    "build:sass": "webpack --mode development --config ./sass-loader/webpack.config.js",
    "build:stylus": "webpack --mode development --config ./stylus-loader/webpack.config.js",
    "build:less": "webpack --mode development --config ./less-loader/webpack.config.js",
    "build:postcss": "webpack --mode development --config ./postcss-loader/webpack.config.js"
  },
  "keywords": [
    "webpack"
  ],
  "author": "@pato-anabalon",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.5.2",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^2.1.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^4.1.0",
    "json-loader": "^0.5.7",
    "less": "^3.10.1",
    "less-loader": "^5.0.0",
    "nib": "^1.1.2",
    "node-sass": "^4.12.0",
    "postcss": "^8.2.10",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "rupture": "^0.7.1",
    "sass-loader": "^7.2.0",
    "style-loader": "0.19.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "url-loader": "^2.1.0",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  }
}
